{
  "$schema": "http://json-schema.org/draft-04/schema",
  "definitions": {
    "HistogramBins": {
      "$schema": "http://json-schema.org/draft-04/schema",
      "title": "Bins",
      "type": "array",
      "items": {
        "type": "object",
        "title": "Bin",
        "description": "Defines a custom object in the array.",
        "propertiesMeta": [
          {
            "name": "name",
            "category": "Analytics",
            "displayName": "Name",
            "displayPriority": 1,
            "description": "Name of Histogram bin.",
            "defaultValue": "",
            "defaultValueInternal": ""
          },
          {
            "name": "value",
            "category": "Analytics",
            "displayName": "Value",
            "displayPriority": 2,
            "description": "Value of Histogram bin.",
            "defaultValue": 0,
            "defaultValueInternal": 0
          }
        ],
        "properties": {
          "name": {
            "anyOf": [
              { "type": "string" },
              { "$ref": "tchmi:framework#/definitions/TcHmiStringSymbol" }
            ]
          },
          "value": {
            "anyOf": [
              { "type": "number" },
              { "$ref": "tchmi:framework#/definitions/TcHmiNumberSymbol" }
            ]
          }
        },
        "required": [ "name, value" ]

      }
    }
  }
}